{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react"
	],
	"jsRules": {},
	"rules": {
		"indent": {
			"options": [
				"tabs"
			]
		},
		"jsx-no-multiline-js": false,
		"no-default-export": true,
		"object-literal-sort-keys": false // TODO: enable this
	},
	"rulesDirectory": []
}
