{
  "name": "money-dashboard",
  "version": "2.0.0",
  "description": "Web application for tracking spending, budgets and investments.",
  "repository": "https://github.com/markormesher/money-dashboard",
  "main": "./dist/app.js",
  "scripts": {
    "build:compile-ts": "tsc -p ./tsconfig.json",
    "build:compile-sass": "node-sass --output-style compressed --source-map true ./src/public/sass/*.scss -o ./dist/public",
    "build:bundle-public-ts": "browserify $(find ./dist/public/js -name '*.js') --plugin tinyify > ./dist/public/bundle.js",
    "build:clean-public-ts": "rm -r ./dist/public/js",
    "build": "npm-run-all build:compile-ts build:compile-sass build:bundle-public-ts build:clean-public-ts"
  },
  "author": "Mark Ormesher",
  "license": "Apache-2.0",
  "dependencies": {
    "async": "^2.6.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "bootstrap-progressbar": "^0.9.0",
    "connect-redis": "^3.3.3",
    "datatables.net": "^1.10.16",
    "datatables.net-bs": "^1.10.16",
    "express": "^4.16.2",
    "express-flash-2": "^1.0.1",
    "express-session": "^1.15.6",
    "gentelella": "^1.4.0",
    "jquery": "^3.3.1",
    "jquery-ui-dist": "^1.12.1",
    "jquery-validation": "^1.17.0",
    "lodash": "^4.17.5",
    "logform": "^1.2.2",
    "moment": "^2.20.1",
    "passport": "^0.4.0",
    "passport-google-oauth2": "^0.1.6",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.28.6",
    "sequelize-typescript": "^0.6.2",
    "toastr": "^2.1.4",
    "uuid": "^3.1.0",
    "winston": "^3.0.0-rc2"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/bootstrap": "^3.3.36",
    "@types/connect-redis": "0.0.7",
    "@types/datatables.net": "^1.10.8",
    "@types/express": "^4.11.1",
    "@types/express-flash-2": "^1.0.3",
    "@types/express-session": "^1.15.8",
    "@types/jquery": "^3.2.17",
    "@types/jquery.validation": "^1.16.4",
    "@types/jqueryui": "^1.12.2",
    "@types/lodash": "^4.14.05",
    "@types/logform": "^1.2.0",
    "@types/node": "^8.5.2",
    "@types/passport": "^0.4.3",
    "@types/passport-google-oauth2": "^0.1.1",
    "@types/pg": "^6.1.45",
    "@types/sequelize": "^4.27.4",
    "@types/toastr": "^2.1.35",
    "@types/uuid": "^3.4.3",
    "browserify": "^14.5.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "tinyify": "^2.4.0",
    "typescript": "2.3.4"
  }
}
