    "history": "^4.7.2",
    "logform": "^2.0.0",
    "mem": "^4.0.0",
    "pg": "^7.4.3",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^6.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-tooltip": "^4.2.5",
