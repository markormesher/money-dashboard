{
  "name": "money-dashboard",
  "sideEffects": false,
  "version": "3.21.2",
  "license": "Apache-2.0",
  "scripts": {
    "// root commands": "//",
    "dev": "run-s dc-build dc-up dc-down",
    "test": "run-s clean build-test start-test",
    "// docker-compose shortcuts": "//",
    "dc-build": "docker-compose build",
    "dc-up": "docker-compose up",
    "dc-down": "docker-compose down",
    "// entry points": "//",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start-dev": "cross-env NODE_ENV=development node ./build/server.js",
    "start-test": "cross-env NODE_ENV=test nyc mocha --exit \"./build/**/*.js\"",
    "// root build jobs": "//",
    "build": "run-s build-tsc build-webpack",
    "build-dev": "run-s build-tsc build-webpack-dev",
    "build-test": "run-s build-tsc",
    "// inner build jobs": "//",
    "build-tsc": "tsc -p ./tsconfig.json",
    "build-webpack": "cross-env NODE_ENV=production webpack",
    "build-webpack-dev": "cross-env NODE_ENV=development webpack",
    "// linters": "//",
    "lint": "run-s lint-src lint-style",
    "lint-fix": "run-s lint-src-fix lint-style-fix",
    "lint-src": "eslint --ext ts,tsx,js,jsx src",
    "lint-src-fix": "eslint --fix --ext ts,tsx,js,jsx src",
    "lint-style": "prettier -c \"src/**/*.{css,scss}\"",
    "lint-style-fix": "prettier --write \"src/**/*.{css,scss}\"",
    "// misc": "//",
    "clean": "rimraf ./build ./coverage ./.nyc_output"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "body-parser": "^1.18.2",
    "bootstrap-scss": "^5.1.3",
    "chart.js": "^4.4.1",
    "compression": "^1.7.3",
    "date-fns": "^2.11.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "logform": "^2.0.0",
    "mem": "^4.0.0",
    "node-cron": "^3.0.0",
    "pg": "^8.7.3",
    "qs": "^6.9.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-tooltip": "^4.2.5",
    "reflect-metadata": "^0.1.10",
    "toastify-js": "^1.12.0",
    "typeorm": "^0.2.9",
    "uuid": "^8.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@dragonlabs/eslint-config": "^0.0.13",
    "@types/async": "^2.0.47",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.7",
    "@types/chai-string": "^1.4.1",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.11.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.1",
    "@types/pg": "^7.4.10",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^5.0.0",
    "@types/react-table": "^6.7.12",
    "@types/sinon": "^10.0.11",
    "@types/toastify-js": "^1.11.1",
    "@types/uuid": "^3.4.3",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-date-fns": "^2.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.3",
    "dts-css-modules-loader": "^2.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "install-peerdeps": "^3.0.3",
    "lockfile-lint": "^4.0.0",
    "mocha": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^2.6.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "sinon": "^13.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "<4.5.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-hot-middleware": "^2.25.3",
    "yarn-deduplicate": "^2.0.0"
  }
}
