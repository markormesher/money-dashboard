{
  "name": "money-dashboard",
  "sideEffects": false,
  "version": "3.2.3",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf ./build",
    "prod": "run-s clean build start",
    "dev": "run-s clean build-dev start-dev",
    "postbuild": "copyfiles -u 2 \"./src/server/config/**\" ./build/server/.",
    "postbuild-dev": "copyfiles -u 2 \"./src/server/config/**\" ./build/server/.",
    "build": "run-p build-client build-server",
    "build-dev": "run-p build-client-dev build-server-dev",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "build-client-dev": "cross-env NODE_ENV=development webpack --progress",
    "build-server": "tsc",
    "build-server-dev": "tsc --diagnostics",
    "start": "cross-env NODE_ENV=production node ./build/server/app.js",
    "start-dev": "cross-env NODE_ENV=development node ./build/server/app.js",
    "lint": "run-p lint-ts lint-scss",
    "lint-scss": "sass-lint -v -c ./sasslint.yml \"./src/client/**/*.scss\"",
    "lint-ts": "tslint -c ./tslint.json \"./src/client/**/*.{ts,tsx}\" \"./src/server/**/*.ts\"",
    "lint-ts-fix": "tslint --fix -c ./tslint.json \"./src/client/**/*.{ts,tsx}\" \"./src/server/**/*.ts\"",
    "test": "run-s test-client test-server",
    "test-client": "run-s test-client-clean test-client-build test-client-run",
    "test-client-clean": "rimraf ./build/client-test",
    "test-client-build": "cross-env NODE_ENV=development TEST=y webpack",
    "test-client-run": "mocha --reporter mocha-circleci-reporter -r ./test/jsdom-setup.js \"./build/client-test/*.js\"",
    "test-server": "echo No server tests yet"
  },
