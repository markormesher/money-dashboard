{
  "name": "money-dashboard",
  "sideEffects": false,
  "version": "3.18.4",
  "license": "Apache-2.0",
  "scripts": {
    "// root commands": "//",
    "dev": "run-s dc-build-dev dc-up-dev dc-down",
    "// docker-compose shortcuts": "//",
    "dc-build-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "dc-up-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "dc-down": "docker-compose down",
    "// entry points": "//",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start-dev": "cross-env NODE_ENV=development node ./build/server.js",
    "start-test": "cross-env NODE_ENV=test nyc mocha -r ./src/test-utils/jsdom-setup.js \"./build/**/*.js\"",
    "// root build jobs": "//",
    "build": "run-s build-tsc build-webpack copy-config",
    "build-dev": "run-s build-dev-tsc build-dev-webpack copy-config",
    "build-test": "run-s build-test-tsc build-test-webpack copy-config",
    "// inner build jobs": "//",
    "build-tsc": "tsc -p ./tsconfig.json",
    "build-dev-tsc": "tsc -p ./tsconfig.json",
    "build-test-tsc": "tsc -p ./tsconfig.test.json",
    "build-webpack": "cross-env NODE_ENV=production webpack -p",
    "build-dev-webpack": "cross-env NODE_ENV=development webpack",
    "build-test-webpack": "cross-env NODE_ENV=test webpack",
    "// linters": "//",
    "lint": "run-s lint-src lint-style",
    "lint-fix": "run-s lint-src-fix lint-style-fix",
    "lint-src": "eslint --ext ts,tsx,js,jsx src",
    "lint-src-fix": "eslint --fix --ext ts,tsx,js,jsx src",
    "lint-style": "prettier -c \"src/**/*.{css,scss}\"",
    "lint-style-fix": "prettier --write \"src/**/*.{css,scss}\"",
    "// misc": "//",
    "clean": "rimraf ./build ./coverage ./.nyc_output",
    "copy-config": "copyfiles -u 1 -e \"./**/*.ts\" \"./src/config/**\" ./build/.",
    "get-private-packages": "./.scripts/get-private-packages.sh"
  },
  "dependencies": {
    "@dragonlabs/redux-cache-key-util": "^0.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/pro-light-svg-icons": "./private-packages/fortawesome-pro-light-svg-icons-5.14.0.tgz",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "async": "^2.6.0",
    "axios": "^0.21.2",
