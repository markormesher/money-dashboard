    "dev": "cross-env NODE_ENV=development node ./build/server/app.js",
    "start": "cross-env NODE_ENV=production node ./build/server/app.js",
    "prebuild": "rimraf ./build",
    "prebuild:dev": "rimraf ./build",
    "postbuild": "copyfiles -u 2 \"./src/server/config/**\" ./build/server/.",
    "postbuild:dev": "copyfiles -u 2 \"./src/server/config/**\" ./build/server/.",
    "build": "yarn build:client && yarn build:server",
    "build:dev": "yarn build:dev:client && yarn build:dev:server",
    "build:server": "tsc",
    "build:dev:server": "tsc --diagnostics",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --config=./webpack.config.prod.js",
    "build:dev:client": "cross-env NODE_ENV=development webpack -p --progress --config=./webpack.config.dev.js",
    "lint": "tslint -c ./tslint.json ./src/server/**/*.ts ./src/client/**/*.ts ./src/client/**/*.tsx",
